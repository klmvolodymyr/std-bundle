{
    "name": "volodymyr-klymniuk/std-bundle",
    "description": "This bundle contains frequently used symfony features",
    "type": "library",
    "license": "MIT-advetising",
    "autoload": {
        "psr-4": {
            "VolodymyrKlymniuk\\StdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    },
    "authors": [
        {
            "name": "Volodymyr Klymniuk",
            "email": "Volodymyr.Klymniuk@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/validator": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "scripts": {
        "test": "phpunit"
    }
}
